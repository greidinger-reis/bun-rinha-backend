{
  "name": "bun-rinha-be",
  "version": "1.0.50",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/app.ts",
    "start": "bun build --outdir=dist --target=bun src/app.ts && bun --bun run dist/app.js ",
    "db:introspect": "drizzle-kit introspect:pg",
    "db:generate": "drizzle-kit generate:pg",
    "clean": "rm -rf dist && rm -rf node_modules",
    "pgtyped": "bunx pgtyped -w -c pgtyped.config.json"
  },
  "dependencies": {
    "@pgtyped/cli": "^2.2.1",
    "@pgtyped/runtime": "^2.1.0",
    "elysia": "latest",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.2.2"
  },
  "module": "src/app.ts"
}