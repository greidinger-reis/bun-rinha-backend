{
  "name": "bun-rinha-be",
  "version": "1.0.50",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "start": "bun run build && bun run --bun dist/app.js",
    "build": "bun build src/app.ts --outdir dist --target bun",
