  },
  "dependencies": {
    "@pgtyped/cli": "^2.2.1",
    "@pgtyped/runtime": "^2.1.0",
    "elysia": "latest",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.2.2"
  },
  "module": "src/app.ts"
}